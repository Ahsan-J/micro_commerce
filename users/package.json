    "@types/koa-graphql": "^0.8.3",
    "@types/koa-mount": "^4.0.0",
    "@types/koa__router": "^8.0.0",
    "@types/lodash": "^4.14.144",
    "@types/mongoose": "^5.7.10",
    "@types/shortid": "0.0.29",
    "@types/supertest": "^2.0.8",
    "jest": "^24.9.0",
    "nodemon": "^2.0.3",
    "supertest": "^4.0.2",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.4"
  },
