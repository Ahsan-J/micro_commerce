{
  "name": "user_module",
  "version": "1.0.0",
  "description": "",
  "main": "src/server.ts",
  "scripts": {
    "start": "ts-node src/server.ts",
    "dev": "nodemon --exec ts-node src/server.ts",
    "test": "jest"
  },
  "dependencies": {
    "@koa/router": "^8.0.2",
    "amqplib": "^0.5.5",
    "graphql": "^14.6.0",
